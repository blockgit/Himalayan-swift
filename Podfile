# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Himalayan-swift' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  #添加约束
  pod 'SnapKit', '~> 5.0.1'
  #分栏控件
  #https://github.com/pujiaxin33/JXSegmentedView.git
  pod 'JXSegmentedView'
  #可定制化的TabbarController(目前使用动画效果)
  #https://github.com/eggswift/ESTabBarController.git
  pod 'ESTabBarController-swift', '~> 2.7'
  #混编OC三方库，自动轮播图
  pod 'SDCycleScrollView', '~> 1.80'
  #URL图片下载缓存
  #https://github.com/onevcat/Kingfisher.git
  #文档http://cocoadocs.org/docsets/Kingfisher/1.6.1/
  pod 'Kingfisher', '~> 5.9.0'

  # Pods for Himalayan-swift

end
